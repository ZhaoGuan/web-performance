{
    "manifest_version": 3,
    "name": "web-performance",
    "description": "web性能测试工具",
    "version": "0.0.2",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "scripting",
        "activeTab",
        "enterprise",
        "system.storage",
        "browsingData"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "options_page": "options.html",
    "devtools_page": "devtools.html"
}